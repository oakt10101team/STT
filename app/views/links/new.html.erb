<div class="pull-right" style="margin: 10px">
  <% if current_user.administrator? %>
  	<%= link_to 'Link Tracks', track_log_path, class: 'btn btn-success' %>
    <%= link_to 'Links', links_path, class: 'btn btn-success' %>
  <% end %>
  <%= link_to "Sign Out #{current_user.full_name}", destroy_user_session_path, method: :delete, class: 'btn btn-warning' %>
</div>
<div class="clearfix"></div>
<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">Import Links</h3>
  </div>
  <div class="panel-body">
		<%= form_tag import_links_path, multipart: true do %>
		  <%= file_field_tag :file %>
		  <br />
		  <%= submit_tag "Import", class: "btn btn-info" %>
		<% end %>
  </div>
</div>